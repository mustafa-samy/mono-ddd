{
  "name": "ng-ddd",
  "version": "1.0.1",
  "description": "A blank schematics",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "copy:files": "mkdir -p dist/domain && cp src/collection.json dist/ && cp src/domain/schema.json dist/domain/ && cp -r src/domain/files dist/domain/ && cp src/format/schema.json dist/format/ && cp -r src/ng-add/schema.json dist/ng-add/",
    "build:full": "yarn build && yarn copy:files",
    "clean": "rm -rf dist",
    "test": "yarn build && jasmine src/**/*_spec.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "Mostafa Sami",
  "license": "MIT",
  "publishConfig": {
    "registry": "http://localhost:4873"
  },
  "keywords": [
    "schematics"
  ],
  "schematics": "./dist/collection.json",
  "ng-add": {
    "save": "devDependencies"
  },
  "dependencies": {
    "@angular-devkit/core": "^19.2.15",
    "@angular-devkit/schematics": "^19.2.15",
    "typescript": "~5.7.2"
  },
  "devDependencies": {
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine": "^5.0.0",
    "rxjs": "^7.8.1",
    "source-map": "0.7.4"
  }
}
